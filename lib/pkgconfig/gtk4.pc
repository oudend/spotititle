prefix=${pcfiledir}/../..
includedir=${prefix}/include
libdir=${prefix}/lib

targets=win32
gtk_binary_version=4.0.0
gtk_host=x86_64-windows

Name: GTK
Description: GTK Graphical UI Library
Version: 4.10.5
Requires: pangowin32 >=  1.50.0, pangocairo >=  1.50.0, gdk-pixbuf-2.0 >=  2.30.0, cairo >=  1.14.0, cairo-gobject >=  1.14.0, graphene-gobject-1.0 >=  1.10.0, gio-2.0 >=  2.72.0
Libs: "-L${libdir}" -lgtk-4
Cflags: "-I${includedir}/gtk-4.0"

